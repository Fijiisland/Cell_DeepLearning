
QScrollBar:vertical {
	background:#464646;
	width:10px;
	margin:0px, 0px, 0px, 0px;
	padding-top:9px;   
    padding-bottom:9px;
}
 
/* 滑块 */
QScrollBar::handle:vertical {
	background:#464646;
	border-radius:2px; 
	min-height:20px;
}
 
/* 底部箭头 */
QScrollBar::add-line:vertical {
	background:#464646;
	height:10px;
	width:10px;
	subcontrol-position:bottom;
}
 
/* 顶部箭头 */
QScrollBar::sub-line:vertical {
	background:#464646;
	height:10px;
	width:10px;
	subcontrol-position:top;
}
 
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical   
{
    background:#262626;
    border-radius:2px;
}